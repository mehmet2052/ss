{
  "version": 2,
  "builds": [
    {
      "src": "tanitim-sitesi/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/tanitim-sitesi/api/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/tanitim-sitesi/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/tanitim-sitesi/index.html"
    }
  ]
}
